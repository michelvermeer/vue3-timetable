{
  "name": "vue3-timetable",
  "description": "A versatile timetable component for Vue 3",
  "private": false,
  "license": "MIT",
  "version": "0.2.2",
  "keywords": [
    "Timetable",
    "Calendar",
    "Events",
    "Schedule",
    "Line-up",
    "Vue",
    "Vue 3",
    "Composition API",
    "Typescript"
  ],
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "author": "Michel Vermeer <info@michelvermeer.com> (https://michelvermeer.com/)",
  "homepage": "https://michelvermeer.com/timetable",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michelvermeer/vue3-timetable.git"
  },
  "peerDependencies": {
    "vue": ">=3"
  }
}
